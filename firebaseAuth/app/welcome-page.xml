<Page xmlns="http://www.nativescript.org/tns.xsd" loaded="onLoaded" navigatedTo="onNavigatedTo">
    <StackLayout class="m-16 p-8" backgroundColor="lightgray">
        <Label text="Login succsessfull! WELCOME!" textWrap="true" />
        <Label text="{{ currentUser.email }}" textWrap="true" />
        <Label text="{{ currentUser.name }}" textWrap="true" />
        <Label text="{{ currentUser.uid }}" textWrap="true" />
        <Image src="{{ currentUser.profileImageURL }}" height="30" width="30" class="img-circle" stretch="none"/>
    </StackLayout>
</Page>